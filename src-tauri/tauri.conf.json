{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "neerajrdp",
    "version": "0.0.0"
  },
  "tauri": {

    "allowlist": {
      "all": true,
      
      "shell": {
        "all": false,
        "open": true
      },"dialog": {
        "all": true, 
        "ask": true, 
        "confirm": true, 
        "message": true, 
        "open": true, 
        "save": true 
      }
      
    },
    "windows": [
      {
        "title": "Raw Dump Tool 6.0",
        "width": 800,
        "height": 600,
        "minWidth":800,
        "minHeight":600
        
      }

    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.RDP",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
